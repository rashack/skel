#!/bin/bash

set -eu

source ~/bin/dzen2-env

e() {
    local v="$1"
    printf "%-20s %s\n" "$v" "${!v}"
}

e DZEN_FONT
e DZEN_OPTS
e DZEN_TIMEBAR_WIDTH
e DZEN_IDLE_X
e DZEN_IDLE_WIDTH
e DZEN_STATUS_X
e DZEN_STATUS_WIDTH
e DZEN_LOAD_X
e DZEN_LOAD_WIDTH
e CONKY_IF
e CONKY_NET
e CONKY_EXTRA
e CONKY_LOAD_STR
e OS_ID
e SCREEN_WIDTH
